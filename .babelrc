{
  "presets": [
    ["env", {
      "targets": {
        "ie": 11
      },
      "modules": false
    }]
  ],
  "plugins": [
    "transform-object-rest-spread"
  ],
  "env": {
    "production": {
      "ignore": [
        "*.spec.js",
        "_*.js"
      ]
    },
    "node": {
      "ignore": [
        "*.spec.js",
        "_*.js"
      ],
      "plugins": ["transform-es2015-modules-commonjs"]
    },
    "test": {
      "plugins": ["transform-es2015-modules-commonjs"]
    }
  }
}
